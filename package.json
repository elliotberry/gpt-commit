{
    "name": "gpt-commit",
    "version": "0.1.2",
    "private": false,
    "description": "Make chatGPT write the important documentation, making your projects more opaque, but saving time.",
    "bin": {
        "gptcommit": "./index.js"
    },
    "keywords": [
        "git",
        "chatGPT",
        "commit",
        "node",
        "entertainment"
    ],
    "os": [
        "linux",
        "darwin"
    ],
    "author": "ɛʟʟɨօȶ b",
    "license": "The Unlicense",
    "main": "./index.js",
    "type": "module",
    "dependencies": {
        "configstore": "^6.0.0",
        "node-fetch": "^3.3.2",
        "prompts": "^2.4.2"
    },
    "devDependencies": {
        "eslint": "^8.56.0",
        "eslint-config-prettier": "^9.1.0",
        "prettier": "^3.2.4"
    },
    "prettier": {
        "trailingComma": "es5",
        "tabWidth": 4,
        "semi": false,
        "singleQuote": true
    },
    "eslintConfig": {
        "root": true,
        "parserOptions": {
            "ecmaVersion": 12,
            "sourceType": "module"
        },
        "extends": [
            "eslint:recommended",
            "prettier"
        ],
        "env": {
            "es2021": true,
            "node": true,
            "es6": true
        },
        "rules": {
            "no-unused-vars": "warn",
            "no-console": "off"
        }
    }
}
